        <omgdi:waypoint x="186" y="168" />
        <omgdi:waypoint x="240" y="168" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_1beouhb_di" bpmnElement="Flow_1beouhb">
        <omgdi:waypoint x="340" y="168" />
        <omgdi:waypoint x="400" y="168" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_0cfwiyd_di" bpmnElement="Flow_0cfwiyd">
