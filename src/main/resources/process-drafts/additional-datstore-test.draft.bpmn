  <process id="additional-datstore-test" name="Additional datstore test" isExecutable="true">
    <startEvent id="startEvent1">
      <extensionElements>
        <jmix:processVariables>
          <jmix:processVariable name="pizzaItems" type="entity-list">
            <jmix:processVariableProperty name="entityName" value="PizzaItem" />
          </jmix:processVariable>
        </jmix:processVariables>
      </extensionElements>
      <outgoing>Flow_0gfg4x7</outgoing>
    </startEvent>
    <sequenceFlow id="Flow_0gfg4x7" sourceRef="startEvent1" targetRef="Activity_0jdyn6l" />
    <sequenceFlow id="Flow_0on9rxg" sourceRef="Activity_0jdyn6l" targetRef="Activity_0b9ivh9" />
    <endEvent id="Event_1rd66xa">
