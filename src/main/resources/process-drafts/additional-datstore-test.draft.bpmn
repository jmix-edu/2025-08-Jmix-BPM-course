      <incoming>Flow_0on9rxg</incoming>
      <outgoing>Flow_0dmfe2v</outgoing>
      <multiInstanceLoopCharacteristics flowable:collection="pizzaItems" flowable:elementVariable="pizzaItems_item" jmix:collectionSource="processVariable" jmix:collectionValue="pizzaItems" />
      <script>println "Pizza from variable name: $pizzaItems_item.name, price is: $pizzaItems_item.price and recipe is: $pizzaItems_item.recipe.name"</script>
    </scriptTask>
    <serviceTask id="Activity_0jdyn6l" name="Load pizzas" flowable:expression="${pizzaService.getPizzaItems()}" flowable:resultVariable="pizzaItems" jmix:taskType="springBean" jmix:beanName="pizzaService">
      <extensionElements>
        <jmix:springBean beanName="pizzaService" methodName="getPizzaItems" />
      </extensionElements>
      <incoming>Flow_0gfg4x7</incoming>
      <outgoing>Flow_0on9rxg</outgoing>
    </serviceTask>
  </process>
