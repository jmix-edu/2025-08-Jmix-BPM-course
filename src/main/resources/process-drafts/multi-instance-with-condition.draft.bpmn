        </extensionElements>
      </sequenceFlow>
      <endEvent id="Event_0faq405">
        <incoming>Flow_15c4zwi</incoming>
      </endEvent>
      <sequenceFlow id="Flow_15c4zwi" sourceRef="Activity_1w1itco" targetRef="Event_0faq405" />
      <scriptTask id="Activity_1w1itco" name="Update global score" scriptFormat="groovy">
        <incoming>Flow_1to5j8p</incoming>
        <outgoing>Flow_15c4zwi</outgoing>
        <script>execution.setVariable("globalScore", globalScore + score)</script>
      </scriptTask>
    </subProcess>
    <sequenceFlow id="Flow_07fowx1" sourceRef="Activity_05o8owo" targetRef="Activity_01m0v7d" />
    <sequenceFlow id="Flow_03g8wmi" sourceRef="Activity_01m0v7d" targetRef="Activity_044cvbj" />
    <scriptTask id="Activity_044cvbj" name="Print average score" scriptFormat="groovy">
      <incoming>Flow_03g8wmi</incoming>
      <outgoing>Flow_1ryst7r</outgoing>
      <script>def resultsNumber = execution.getVariable("instancesQty")
def avg = globalScore/resultsNumber
println "Score AVG: $avg"</script>
    </scriptTask>
    <endEvent id="Event_1ayorgt">
