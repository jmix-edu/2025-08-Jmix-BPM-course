<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:flowable="http://flowable.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:jmix="http://jmix.io/schema/bpm/bpmn" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.flowable.org/processdef" xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/2.0/20100501/BPMN20.xsd">
  <process id="pizza-process-with-user-provider" name="Pizza process - provider" isExecutable="true" flowable:candidateStarterUsers="peter" jmix:candidateStarterUsersValue="peter">
    <startEvent id="startEvent1" name="Feed developer">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="message" caption="Message for developer" type="string" editable="true" required="false" />
            <jmix:formField id="pizzaEater" caption="Pizza eater" type="entity" editable="true" required="false">
              <jmix:formFieldProperty name="entityName" value="User" />
              <jmix:formFieldProperty name="uiComponent" value="comboBox" />
              <jmix:formFieldProperty name="lookupScreen" value="User.list" />
              <jmix:formFieldProperty name="query" value="select u from User u where u.position = &#39;developer&#39;" />
            </jmix:formField>
          </jmix:formFields>
        </jmix:formData>
      </extensionElements>
      <outgoing>Flow_1dhd1js</outgoing>
    </startEvent>
    <sequenceFlow id="Flow_1dhd1js" sourceRef="startEvent1" targetRef="Activity_1st2asf" />
    <sequenceFlow id="Flow_0a3k7zc" sourceRef="Activity_1st2asf" targetRef="Gateway_0bebfvn" />
    <sequenceFlow id="Flow_1ynq0vv" sourceRef="Activity_0q5u49u" targetRef="Gateway_0xd915y" />
    <endEvent id="Event_1evk2cx" name="Enjoy">
      <incoming>Flow_0fjp7b6</incoming>
    </endEvent>
    <sequenceFlow id="Flow_0fjp7b6" sourceRef="Activity_06n2ks0" targetRef="Event_1evk2cx">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" />
      </extensionElements>
    </sequenceFlow>
    <userTask id="Activity_1st2asf" name="Choose pizza" flowable:assignee="${pizzaEater.username}" jmix:assigneeSource="processVariable" jmix:assigneeValue="pizzaEater">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="message" caption="Message" type="string" editable="false" required="false" />
            <jmix:formField id="pizzaItem" caption="" type="platformEnum" editable="true" required="true">
              <jmix:formFieldProperty name="enumClass" value="com.company.bpmcourse.entity.PizzaItem" />
            </jmix:formField>
            <jmix:formField id="specialRequirements" caption="" type="multilineString" editable="true" required="false" />
          </jmix:formFields>
          <jmix:formOutcomes>
            <jmix:formOutcome id="pizza_selected" caption="Pizza selected" icon="THUMBS_UP" />
            <jmix:formOutcome id="not_hungry" caption="Not hungry" icon="THUMBS_DOWN" />
          </jmix:formOutcomes>
        </jmix:formData>
      </extensionElements>
      <incoming>Flow_1dhd1js</incoming>
      <outgoing>Flow_0a3k7zc</outgoing>
    </userTask>
    <userTask id="Activity_0q5u49u" name="Approve order" flowable:assignee="${randomRobinAccountantProvider.getUser()}" jmix:assigneeSource="userProvider" jmix:assigneeValue="randomRobinAccountantProvider">
      <extensionElements>
        <jmix:springBean beanName="randomRobinAccountantProvider" methodName="getUser" />
      </extensionElements>
      <incoming>Flow_07l389m</incoming>
      <outgoing>Flow_1ynq0vv</outgoing>
    </userTask>
    <userTask id="Activity_06n2ks0" name="Place order" flowable:assignee="${initiator.username}" jmix:assigneeSource="processVariable" jmix:assigneeValue="initiator">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="approverMessage" caption="Approver message" type="string" editable="false" required="false" />
            <jmix:formField id="pizzaEater" caption="Pizza eater" type="entity" editable="false" required="false">
              <jmix:formFieldProperty name="entityName" value="User" />
              <jmix:formFieldProperty name="uiComponent" value="entityPicker" />
              <jmix:formFieldProperty name="lookupScreen" value="User.list" />
            </jmix:formField>
            <jmix:formField id="pizzaItem" caption="Pizza item" type="platformEnum" editable="false" required="false">
              <jmix:formFieldProperty name="enumClass" value="com.company.bpmcourse.entity.PizzaItem" />
            </jmix:formField>
            <jmix:formField id="specialRequirements" caption="Special requirements" type="multilineString" editable="false" required="false" />
            <jmix:formField id="deliveryNumber" caption="Delivery number" type="string" editable="true" required="true" />
          </jmix:formFields>
        </jmix:formData>
      </extensionElements>
      <incoming>Flow_1cz6zpq</incoming>
      <outgoing>Flow_0fjp7b6</outgoing>
    </userTask>
    <exclusiveGateway id="Gateway_0bebfvn" default="Flow_07l389m">
      <incoming>Flow_0a3k7zc</incoming>
      <outgoing>Flow_07l389m</outgoing>
      <outgoing>Flow_08950d2</outgoing>
    </exclusiveGateway>
    <sequenceFlow id="Flow_07l389m" name="Selected" sourceRef="Gateway_0bebfvn" targetRef="Activity_0q5u49u" />
    <exclusiveGateway id="Gateway_0xd915y" default="Flow_1cz6zpq">
      <incoming>Flow_1ynq0vv</incoming>
      <outgoing>Flow_1cz6zpq</outgoing>
      <outgoing>Flow_1845bbf</outgoing>
    </exclusiveGateway>
    <sequenceFlow id="Flow_1cz6zpq" name="Approved" sourceRef="Gateway_0xd915y" targetRef="Activity_06n2ks0" />
    <endEvent id="Event_1r8ysml">
      <incoming>Flow_08950d2</incoming>
    </endEvent>
    <sequenceFlow id="Flow_08950d2" name="Not hungry" sourceRef="Gateway_0bebfvn" targetRef="Event_1r8ysml">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="anyoneCompleted" userTaskId="Activity_1st2asf" userTaskOutcome="not_hungry" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_1st2asf_result') != null &amp;&amp; bpm_UserTaskResults.containsOutcome(execution.getVariable('Activity_1st2asf_result'), 'not_hungry')}</conditionExpression>
    </sequenceFlow>
    <endEvent id="Event_07tlicy">
      <incoming>Flow_1845bbf</incoming>
    </endEvent>
    <sequenceFlow id="Flow_1845bbf" name="Rejected" sourceRef="Gateway_0xd915y" targetRef="Event_07tlicy">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="anyoneCompleted" userTaskId="Activity_0q5u49u" userTaskOutcome="rejected" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_0q5u49u_result') != null &amp;&amp; bpm_UserTaskResults.containsOutcome(execution.getVariable('Activity_0q5u49u_result'), 'rejected')}</conditionExpression>
    </sequenceFlow>
  </process>
