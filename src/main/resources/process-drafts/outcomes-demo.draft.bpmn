            <jmix:processVariableProperty name="entityName" value="User" />
          </jmix:processVariable>
        </jmix:processVariables>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="approvers" caption="Approvers" type="entity-list" editable="true" required="true">
              <jmix:formFieldProperty name="entityName" value="User" />
              <jmix:formFieldProperty name="uiComponent" value="entityPicker" />
            </jmix:formField>
          </jmix:formFields>
        </jmix:formData>
      </extensionElements>
      <outgoing>Flow_09fqulw</outgoing>
    </startEvent>
    <sequenceFlow id="Flow_09fqulw" sourceRef="startEvent1" targetRef="Activity_approve">
      <extensionElements>
        <jmix:conditionDetails conditionSource="expression" />
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="Flow_1aax331" sourceRef="Activity_approve" targetRef="Activity_1u0g1jo" />
