      <outgoing>Flow_1aln49d</outgoing>
      <outgoing>Flow_1preuj5</outgoing>
      <outgoing>Flow_0xwut41</outgoing>
      <outgoing>Flow_1gco5so</outgoing>
      <outgoing>Flow_1wyhg7l</outgoing>
    </inclusiveGateway>
    <endEvent id="Event_1wna3lq">
      <incoming>Flow_1aln49d</incoming>
    </endEvent>
    <sequenceFlow id="Flow_1aln49d" name="All yes" sourceRef="Gateway_0uc2r23" targetRef="Event_1wna3lq">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="everyoneCompleted" userTaskId="Activity_approve" userTaskOutcome="yes" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_approve_result') != null &amp;&amp; bpm_UserTaskResults.getOutcomesCount(execution.getVariable('Activity_approve_result'),'yes') == bpm_UserTaskResults.getOutcomesCount(execution.getVariable('Activity_approve_result'))}</conditionExpression>
    </sequenceFlow>
    <endEvent id="Event_1s9yb7j">
      <incoming>Flow_1preuj5</incoming>
    </endEvent>
    <sequenceFlow id="Flow_1preuj5" name="All no" sourceRef="Gateway_0uc2r23" targetRef="Event_1s9yb7j">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="everyoneCompleted" userTaskId="Activity_approve" userTaskOutcome="no" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_approve_result') != null &amp;&amp; bpm_UserTaskResults.getOutcomesCount(execution.getVariable('Activity_approve_result'),'no') == bpm_UserTaskResults.getOutcomesCount(execution.getVariable('Activity_approve_result'))}</conditionExpression>
    </sequenceFlow>
    <endEvent id="Event_11jljhx">
      <incoming>Flow_0xwut41</incoming>
    </endEvent>
    <sequenceFlow id="Flow_0xwut41" name="No no" sourceRef="Gateway_0uc2r23" targetRef="Event_11jljhx">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="nooneCompleted" userTaskId="Activity_approve" userTaskOutcome="no" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_approve_result') != null &amp;&amp; !bpm_UserTaskResults.containsOutcome(execution.getVariable('Activity_approve_result'),'no')}</conditionExpression>
    </sequenceFlow>
    <userTask id="Activity_approve" name="Approve" flowable:assignee="${approvers_item.username}" jmix:assigneeSource="expression" jmix:assigneeValue="${approvers_item.username}">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formOutcomes>
            <jmix:formOutcome id="yes" caption="Yes" />
            <jmix:formOutcome id="no" caption="No" />
            <jmix:formOutcome id="maybe" caption="Maybe" />
          </jmix:formOutcomes>
        </jmix:formData>
      </extensionElements>
      <incoming>Flow_09fqulw</incoming>
      <outgoing>Flow_1aax331</outgoing>
      <multiInstanceLoopCharacteristics flowable:collection="approvers" flowable:elementVariable="approvers_item" jmix:collectionSource="processVariable" jmix:collectionValue="approvers" />
    </userTask>
    <sequenceFlow id="Flow_0kn4tn9" sourceRef="Activity_1u0g1jo" targetRef="Activity_0vcazqg" />
    <sequenceFlow id="Flow_04ftzzr" sourceRef="Activity_0vcazqg" targetRef="Gateway_0uc2r23" />
    <serviceTask id="Activity_1u0g1jo" name="Get outcomes string" flowable:expression="${outcomesContainerReaderDelegate.execute(execution)}" flowable:resultVariable="outcomes" jmix:taskType="${outcomesContainerReaderDelegate.execute(execution)}" jmix:beanName="outcomesContainerReaderDelegate">
      <extensionElements>
        <jmix:springBean beanName="outcomesContainerReaderDelegate" methodName="execute">
          <jmix:methodParam name="execution" type="org.flowable.engine.delegate.DelegateExecution" isVariable="true">execution</jmix:methodParam>
        </jmix:springBean>
      </extensionElements>
      <incoming>Flow_1aax331</incoming>
      <outgoing>Flow_0kn4tn9</outgoing>
    </serviceTask>
    <userTask id="Activity_0vcazqg" name="See outcomes" flowable:assignee="${initiator.username}" jmix:assigneeSource="processVariable" jmix:assigneeValue="initiator">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="outcomes" caption="Outcomes" type="string" editable="false" required="false" />
          </jmix:formFields>
        </jmix:formData>
      </extensionElements>
      <incoming>Flow_0kn4tn9</incoming>
      <outgoing>Flow_04ftzzr</outgoing>
    </userTask>
    <sequenceFlow id="Flow_1gco5so" name="Anyone maybe" sourceRef="Gateway_0uc2r23" targetRef="Event_0epndqw">
      <extensionElements>
        <jmix:conditionDetails conditionSource="userTaskOutcome" conditionType="anyoneCompleted" userTaskId="Activity_approve" userTaskOutcome="maybe" />
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression">${execution.getVariable('Activity_approve_result') != null &amp;&amp; bpm_UserTaskResults.containsOutcome(execution.getVariable('Activity_approve_result'), 'maybe')}</conditionExpression>
    </sequenceFlow>
    <endEvent id="Event_0epndqw">
      <incoming>Flow_1gco5so</incoming>
    </endEvent>
    <endEvent id="Event_0myyz70">
      <incoming>Flow_1wyhg7l</incoming>
    </endEvent>
    <sequenceFlow id="Flow_1wyhg7l" name="No suitable result" sourceRef="Gateway_0uc2r23" targetRef="Event_0myyz70">
      <extensionElements>
        <jmix:conditionDetails conditionSource="expression" />
      </extensionElements>
    </sequenceFlow>
